<!DOCTYPE html>
<html>
<head>
  <title>To-Do List</title>
</head>
<body>
  <h1>To-Do List</h1>
  <ul>
    <% todoList.forEach(task => { %>
      <li><%= task.task %></li>
    <% }); %>
  </ul>
  <form action="/add" method="post">
    <input type="text" name="task" placeholder="Add a new task" />
    <button type="submit">Add</button>
  </form>
</body>
</html>

